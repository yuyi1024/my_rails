<% if @action == 'subcat' %>
	$('#subcat_box').replaceWith("<%= j render partial: 'cat_selectbox',locals: { subcategories: @subcategories } %>");

<% elsif @action == 'search' %>
	$('.product_list').replaceWith("<%= j render partial: 'items', locals: { products: @products } %>");
	$('.count span').text('<%= @products.length %>');
<% end %>